    "tokenColors": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#7F8BAD80",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "meta.parameters comment.block",
        "settings": {
          "foreground": "#7F8BAD",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["variable.other.constant.object", "variable.other.readwrite.alias", "meta.import variable.other.readwrite"],
        "settings": {
          "foreground": "#98d1ef"
        }
      },
      {
        "scope": ["variable.other", "support.type.object"],
        "settings": {
          "foreground": "#E1DEE3"
        }
      },
      {
        "scope": ["variable.other.object.property", "variable.other.property", "support.variable.property", "support.type.primitive"],
        "settings": {
          "foreground": "#E1DEE3"
        }
      },
      {
        "scope": [".method", "string.unquoted", "meta.object.member"],
        "settings": {
          "foreground": "#98d1ef"
        }
      },
      {
        "scope": ["variable - meta.import", "variable.other.readwrite.alias", "constant.other.placeholder", "meta.object-literal.key-meta.object.member"],
        "settings": {
          "foreground": "#E1DEE3"
        }
      },
      {
        "scope": ["keyword.control.flow"],
        "settings": {
          "foreground": "#FF6633",
           "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword.operator.new", "keyword.control.new", "keyword.control.default"],
        "settings": {
          "foreground": "#FF6633",
        }
      },
      {
        "scope": [
          "variable.language.this", "storage.modifier.async", "storage.modifier", "variable.language.super"
        ],
        "settings": {
          "foreground": "#FF6633",
        }
      },
      {
        "scope": [         
          "support.class.error", 
          "keyword.control.trycatch", 
          "keyword.operator.expression.delete", 
          "keyword.operator.expression.void", 
          "keyword.operator.void", 
          "keyword.operator.delete",
          "constant.language.null",
          "constant.language.boolean.false",
